extends ../layouts/layout.pug
block content 
    +breadCrumb("heading", "contact us", "index.html", "home", "contact")
    section(class="contact")
        form(action="#", method="get")
            h5 get in touch
            label(for="name") name 
            input(type="text", name="name", id="name")
            label(for="number") number
            input(type="number", name="number", id="number")
            label(for="email") email 
            input(type="email", name="email", id="email")
            label(for="message") message
            textarea(name="message", id="message", cols="30", rows="10")
            button(type="submit", class="btn") send message 
        iframe(
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d7205.308978945664!2d30.548602772904268!3d25.449812335117116!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x144769db3c483fdd%3A0xf31bd4f894269ed4!2sMAX%20PLAYSTATION!5e0!3m2!1sar!2seg!4v1640200022431!5m2!1sar!2seg", 
            class="map"
        )